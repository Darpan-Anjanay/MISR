	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "Login - Get JWT Tokens",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let data = pm.response.json();\r",
									"\r",
									"if (data.access) {\r",
									"    pm.collectionVariables.set(\"access_token\", data.access);\r",
									"  \r",
									"}\r",
									"\r",
									"if (data.refresh) {\r",
									"    pm.collectionVariables.set(\"refresh_token\", data.refresh);\r",
									"    \r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"{{username}}\",\n    \"password\": \"{{password}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/accesstoken/",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"accesstoken",
								""
							]
						}
					},
					"response": []
				},
				{
					"name": "Refresh Access Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let data = pm.response.json();\r",
									"\r",
									"if (data.access) {\r",
									"    pm.collectionVariables.set(\"access_token\", data.access);\r",
									"  \r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"refresh\": \"{{refresh_token}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/refreshtoken/",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"refreshtoken",
								""
							]
						}
					},
					"response": []
				}
			]
		},
